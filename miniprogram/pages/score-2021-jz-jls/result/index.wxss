page {
  background: #fff;
  padding-bottom: 240rpx;
}

.banner_bk {
  width: 100vw;
  display: block;
  background: #4db4b4;
}

.title {
  margin: -320rpx auto 0;
  text-align: center;
  color: #fff;
  font-size: 150%;
}

.my_tip {
  width: 603rpx;
  background: #ff7d0e;
  border-radius: 37rpx;
  text-align: center;
  line-height: 74rpx;
  padding: 5rpx 0;
  margin: 20rpx auto 0;
}

.my_tip text {
  display: inline-block;
  font-size: 26rpx;
  line-height: 40rpx;
  color: #fff;
  vertical-align: middle;
  padding: 15rpx;
}

.nav_top {
  width: 100%;
  height: 44rpx;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  padding-top: 60rpx;
  padding-bottom: 30rpx;
}

.nav_top .nav_back {
  width: 24rpx;
  height: 44rpx;
  margin-left: 24rpx;
}

.nav_top .nav_back image {
  display: block;
  width: 24rpx;
  height: 44rpx;
}

.nav_top .nav_title {
  width: 637rpx;
  height: 44rpx;
  line-break: 44rpx;
  font-size: 32rpx;
  font-weight: bold;
  text-align: center;
  color: #fff;
}

.result_num {
  width: 702rpx;
  margin: 0 auto;
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  height: 73rpx;
  line-height: 73rpx;
  padding: 10rpx 0;
}

.result_nux {
  font-size: 34rpx;
  font-weight: bold;
  color: #f5eeb8;
  padding-left: 90rpx;
  display: inline-block;
}

.result_num navigator{
  margin-right:100rpx;
}
.change_btn {
  font-size: 34rpx;
  color: #fff;
  background: none;
  display: inline-block;
  padding: 0 20rpx;
  line-height: 34rpx;
  height: 34rpx;
  margin-top: 21rpx;
}

.change_btn::after {
  background: none;
  border: none;
}

.nav_wrap {
  width: 700rpx;
  display: flex;
  display: -webkit-flex;
  margin: 0 auto;
  flex-direction: row;
  justify-content: space-between;
  flex-wrap: wrap;
  padding-top: 30rpx;
}

.nav_wrap .nav_item {
  width: 130rpx;
  height: 60rpx;
  border: 1rpx solid #c2c2c2;
  line-height: 60rpx;
  text-align: center;
  font-size: 26rpx;
  color: #3b444e;
  background-color: #fff;
  margin-bottom: 15rpx;
  border-radius: 30rpx;
  transition: all 0.4s;
}

.nav_wrap .nav_item.nav_itemx {
  border: 1px solid #ededf1;
  background-color: #ededf1;
}

.nav_wrap .nav_item.active {
  background-color: #d32423;
  border: 1px solid #d32423;
  color: #fff;
}


.tip_cvover {
  width: 100%;
  height: 227rpx;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 999;
  background: rgba(0, 0, 0, 0.6);
  border-radius: 10rpx 10rpx 0 0;
  padding-top: 1rpx;
  z-index: 9999999999;
}

.tip_cvover.active {
  -webkit-animation: bounceInUp 1s;
  animation: bounceInUp 1s;
}

.tip_cvover .tip_con {
  width: 700rpx;
  height: 92rpx;
  background: #d32423;
  margin: 0 auto;
  border-radius: 15rpx;
  margin-top: 66rpx;
  display: flex;
  display: -webkit-flex;
  justify-content: flex-start;
}

.tip_cvover .tip_con .close_tip {
  width: 39rpx;
  height: 39rpx;
  padding: 26.5rpx;
  margin-left: 24rpx;
}

.tip_cvover .tip_con .tip_text {
  width: 469rpx;
  height: 92rpx;
  font-size: 32rpx;
  color: #fff;
  text-align: center;
  line-height: 92rpx;
}

.tip_cvover .tip_con .tip_icon {
  width: 22rpx;
  height: 38rpx;
  padding: 26.5rpx;
}

.nav_title {
  font-size: 28rpx;
  color: #3b444e;
  line-height: 28rpx;
  width: 700rpx;
  margin: 0 auto;
  padding-top: 50rpx;
  font-weight: bold;
}


.resultWrap {
  width: 700rpx;
  margin: 0 auto;
}

.resultWrap .resultLine {
  width: 100%;
  height: 66rpx;
  border-bottom: 1rpx solid #c6c6c6;
  overflow: hidden;
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  flex-wrap: nowrap;
}

.resultWrap .resultLine.resultLine1 {
  background: #d32423;
}

.resultWrap .resultLine.resultLine1 .resultLineItem {
  background: #d32423;
  color: #fff;
  border-right: 1rpx solid #d32423;
  font-size: 24rpx;
}

.resultWrap .resultLine.resultLine1 .resultLineItem5 {
  border-right: none;
}

.resultWrap .resultLine .resultLineItem {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  text-align: center;
  background-color: #fff;
  border-right: 1rpx solid #c6c6c6;
  line-height: 66rpx;
  font-size: 24rpx;
}

.resultWrap .resultLine .resultLineItem1 {
  flex: 20;
}

.resultWrap .resultLine .resultLineItem2 {
  flex: 45;
}

.resultWrap .resultLine .resultLineItem4 {
  flex: 15;
}

.resultWrap .resultLine .resultLineItem5 {
  flex: 20;
  border-right: none;
}

.resultWrap .resultLine .resultLineItem5 .btns {
  width: 104rpx;
  height: 25rpx;
}

.page_cover {
  position: fixed;
  left: 0;
  top: 0;
  bottom: 0;
  right: 0;
  background: rgba(0, 0, 0, 0.5);
  z-index: 99999;
}

.cover_con {
  width: 680rpx;
  padding: 20rpx;
  background: #c00101;
  border-radius: 30rpx;
  position: fixed;
  left: 50%;
  top: 13%;
  margin-left: -360rpx;
  z-index: 9999999;
}

.knowBtns {
  width: 400rpx;
  height: 80rpx;
  background: #c00101;
  border-radius: 20rpx;
  line-height: 80rpx;
  text-align: center;
  position: absolute;
  bottom: -100rpx;
  left: 50%;
  margin-left: -200rpx;
  color: #fff;
  font-size: 36rpx;
}


.result_item {
  width: 620rpx;
  margin: 0 auto;
  border-radius: 20rpx;
  background: #fff;
  border-radius: 15rpx;
  -moz-box-shadow: 0rpx 0rpx 7rpx #d6d7d3;
  -webkit-box-shadow: 0rpx 0rpx 7rpx #d6d7d3;
  box-shadow: 0rpx 0rpx 7rpx #d6d7d3;
  padding: 30rpx;
  position: relative;
  margin-bottom: 30rpx;
}

.result_code {
  display: block;
  font-size: 40rpx;
  font-weight: bold;
  color: #3b444e;
  line-height: 100rpx;
  text-align: center;
}

