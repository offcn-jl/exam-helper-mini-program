.result_detail {
  display: block;
  font-size: 40rpx;
  /* color: #999999; */
  line-height: 49rpx;
  background: #f4f8f9;
  padding: 20rpx;
  border-radius: 10rpx;
  text-align: center;
  color: #f10;
}

.item_group {
  width: 100%;
  padding-top: 20rpx;
}

.item-group-wrap {
  max-height: 0;
  transition: all 1s;
  overflow: hidden;
}

.item-group-wrap.active {
  max-height: 2000rpx;
  transition: all 1s;
  overflow: hidden;
}

.item_group .item_line {
  display: flex;
  justify-content: flex-start;
  margin-bottom: 14rpx;
  flex-direction: row;
  flex-wrap: wrap;
  font-size: 36rpx;
  line-height: 60rpx;
}

.item_group .item_line label {
  color: #969799;
  display: inline-block;
  margin-right: 10rpx;
  /* 文本两端对齐 */
  text-align-last: justify;
  width: 160rpx;
}

.jiantou {
  width: 100%;
  position: absolute;
  bottom: 20rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.jiantou image {
  display: block;
  width: 32rpx;
  height: 21rpx;
  transform: rotate(180deg);
}

.jiantou image.active {
  transition: all 0.2s;
  transform: rotate(0);
}

.item_group .item_line text {
  color: #46504f;
  display: inline-block;
}

.item_group .item_line.item_line_fs {
  height: 50rpx;
  line-height: 50rpx;
}

.item_group .item_line.item_line_fs label {
  line-height: 50rpx;
}

.item_group .item_line.item_line_fx label {
  line-height: 36rpx;
}

.item_group .item_line.item_line_fs text {
  display: block;
  font-weight: bold;
  text-align: center;
  color: #d32423;
  font-size: 28rpx;
  line-height: 50rpx;
}

.show_more {
  width: 26rpx;
  height: 23rpx;
  display: block;
  margin: 0 auto;
  padding: 30rpx;
}

.add_btn {
  width: 180rpx;
  height: 50rpx;
  background: #d32423;
  line-height: 50rpx;
  position: absolute;
  right: 30rpx;
  top: 20rpx;
  padding: 0;
  text-align: center;
  color: #fff;
  border-radius: 27rpx;
  font-size: 25rpx;
}

.add_btn::after {
  border: none;
}
