<navigation id='navigation' title="中公考试助手"></navigation>

<image class="ad-background" mode="widthFix" src="{{background}}">
  <button class="ad-button" bindtap="navigate" style="color: {{buttonContentColor}}; background: {{buttonColor}}">{{buttonContent}}</button>
</image>

<view class="suffix">{{suffix}}</view>

<navigator url="/pages/index/index?scene={{suffix}}" class="unsubscribe" hover-class="unsubscribe-hover">不想继续收到此类推送？点击退订！</navigator>