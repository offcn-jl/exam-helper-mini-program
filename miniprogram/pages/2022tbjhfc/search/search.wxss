}

.fenshuyc .fenshuBox .phbtit2 .redcss {
    color: red;
}

.shuom {
    width: 680rpx;
    margin: 20rpx auto 0;
    color: #fff;
    font-size: 22rpx;
    text-align: center;
}

.fenshuyc .fenshuBox .phbtit2 view:nth-child(1),.fenshuyc .fenshuBox .phbtit2 view:nth-child(2),.phbtit view:nth-child(1),.phbtit view:nth-child(2) {
    width: 150rpx;
}