/* pages/gk/2023/enrollments/index.wxss */
@import "../../../tgjs/2022/shaif/index.wxss";

page {
    background: #0260e4;
}

.container {
    margin-top: -1250rpx;
}

.banner_bk {
    margin-top: 0;
}

.select_wrap {
    background: white;
}

.cover {
    position: fixed;
    left: 0;
    top: 0;
    right: 0;
    bottom: 0;
    background: #000;
    opacity: 0.7;
    z-index: 100;
}

.popup {
    position: fixed;
    top: 20vh;
    left: 5vw;
    width: 90vw;
    background: white;
    z-index: 101;
    border-radius: 50rpx;
}

.popup .close {
    position: fixed;
    top: 21.5vh;
    right: 10.5vw;
    font-size: 25px;
    font-weight: 900;
}

.popup .header {
    font-size: 50rpx;
    text-align: center;
    margin: 30rpx auto;
}

.popup .item {
    font-size: 35rpx;
    padding: 10rpx 45rpx;
}

.popup .item:last-child {
    padding-bottom: 40rpx;
}

.popup .item .title{
    color: gray;
    display: inline;
}
